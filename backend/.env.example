# Fortium Payouts Backend - Environment Variables

# Database (Supabase PostgreSQL)
DATABASE_URL=postgres://user:password@host:5432/database

# Google OAuth
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_ALLOWED_DOMAIN=fortiumpartners.com
SESSION_SECRET=generate-a-random-string

# Base URL (for OAuth callbacks)
BASE_URL=http://localhost:8000

# Logging
LOG_LEVEL=info
NODE_ENV=development

# fpqbo API
FPQBO_API_URL=https://fpqbo.example.com
FPQBO_API_KEY_US=
FPQBO_API_KEY_CA=

# PartnerConnect API
PARTNERCONNECT_API_URL=https://api.partnerconnect.example.com
PARTNERCONNECT_CLIENT_ID=
PARTNERCONNECT_CLIENT_SECRET=
PARTNERCONNECT_AUTH0_DOMAIN=
PARTNERCONNECT_AUDIENCE=

# Bill.com API (US payments)
BILLCOM_API_URL=https://api.bill.com
BILLCOM_API_KEY=

# Wise API (Canada payments)
WISE_API_URL=https://api.wise.com
WISE_API_KEY=
